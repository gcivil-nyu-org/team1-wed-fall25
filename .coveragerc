[run]
source = .
omit =
    */migrations/*
    */tests/*
    */test_*.py
    */__pycache__/*
    */venv/*
    */env/*
    */virtualenv/*
    manage.py
    core/wsgi.py
    core/asgi.py
    */management/commands/import_art_data.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if settings.DEBUG
    pass
    
precision = 2
show_missing = True

[html]
directory = htmlcov
